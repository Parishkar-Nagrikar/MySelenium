    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>
    </listeners>

    <test name="DoctorPlus Tests" >
        <classes>
            <class name="DoctorPlus.TestNG.DoctorPlusTestNg" />
        </classes>
    </test>

    <test name="Selenium Guru Test" >
        <classes>
            <class name="DoctorPlus.TestNG.SeleniumGuruTestNg" />
        </classes>
    </test>
</suite>
